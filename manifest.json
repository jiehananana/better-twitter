{
	"name": "Focus mode",
	"description": "allow us to focus af, no more mindless scroll",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_icon": "hello_extensions.png"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": ["tabs", "activeTab", "scripting"],
	"host_permissions": ["http://*/*", "https://*/*"]
}
